r\/documentation\/SlothCreator","kind":"symbol","role":"collection","type":"topic","images":[{"type":"icon","identifier":"slothCreator-icon.png"}]},"doc://SlothCreator/documentation/SlothCreator/Sloth/power-swift.property":{"title":"power","url":"\/documentation\/slothcreator\/sloth\/power-swift.property","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"text":"power","kind":"identifier"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:12SlothCreator0A0V5PowerO","text":"Power"}],"identifier":"doc:\/\/SlothCreator\/documentation\/SlothCreator\/Sloth\/power-swift.property","abstract":[{"text":"The power of the sloth.","type":"text"}],"role":"symbol","type":"topic","kind":"symbol"},"slothCreator-icon.png":{"variants":[{"url":"\/images\/slothCreator-icon@2x.png","traits":["2x","light"]}],"alt":"A technology icon representing the SlothCreator framework.","identifier":"slothCreator-icon.png","type":"image"},"doc://SlothCreator/documentation/SlothCreator/Sloth/Food":{"title":"Sloth.Food","url":"\/documentation\/slothcreator\/sloth\/food","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"Food","kind":"identifier"}],"identifier":"doc:\/\/SlothCreator\/documentation\/SlothCreator\/Sloth\/Food","abstract":[{"text":"Food that a sloth can consume.","type":"text"}],"role":"symbol","navigatorTitle":[{"text":"Food","kind":"identifier"}],"type":"topic","kind":"symbol"},"slothy-card.png":{"variants":[{"url":"\/images\/slothy-card@2x.png","traits":["2x","light"]}],"alt":"Two screenshots showing the Slothy app. The first screenshot shows a sloth map and the second screenshot shows a sloth power picker.","identifier":"slothy-card.png","type":"image"}}}